[32m+[m[32m'use client';[m
[32m+[m
[32m+[m[32mimport { useState, useEffect } from 'react';[m
[32m+[m[32mimport Header from '../../../components/admin/dashboard/Header';[m
[32m+[m[32mimport Sidebar from '../../../components/admin/dashboard/Sidebar';[m
[32m+[m[32mimport { withAdminAuth } from '../../../components/auth/withAuth';[m
[32m+[m[32mimport {[m
[32m+[m[32m  usersApi,[m
[32m+[m[32m  type UserProfile,[m
[32m+[m[32m  type ChangePasswordPayload,[m
[32m+[m[32m} from '../../../lib/api';[m
[32m+[m
[32m+[m[32mfunction AdminSettingsPage() {[m
[32m+[m[32m  const [isMobileMenuOpen, setIsMobileMenuOpen] = useState(false);[m
[32m+[m[32m  const [profile, setProfile] = useState<UserProfile | null>(null);[m
[32m+[m[32m  const [loading, setLoading] = useState(true);[m
[32m+[m[32m  const [savingProfile, setSavingProfile] = useState(false);[m
[32m+[m[32m  const [changingPassword, setChangingPassword] = useState(false);[m
[32m+[m[32m  const [passwordForm, setPasswordForm] = useState<ChangePasswordPayload>({[m
[32m+[m[32m    currentPassword: '',[m
[32m+[m[32m    newPassword: '',[m
[32m+[m[32m    confirmationPassword: '',[m
[32m+[m[32m  });[m
[32m+[m[32m  const [message, setMessage] = useState<string | null>(null);[m
[32m+[m[32m  const [error, setError] = useState<string | null>(null);[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    const fetchMe = async () => {[m
[32m+[m[32m      setLoading(true);[m
[32m+[m[32m      const me = await usersApi.getMe();[m
[32m+[m[32m      setProfile(me);[m
[32m+[m[32m      setLoading(false);[m
[32m+[m[32m    };[m
[32m+[m[32m    fetchMe();[m
[32m+[m[32m  }, []);[m
[32m+[m
[32m+[m[32m  const handleProfileChange = (field: keyof UserProfile, value: string) => {[m
[32m+[m[32m    if (!profile) return;[m
[32m+[m[32m    setProfile({ ...profile, [field]: value } as UserProfile);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleSaveProfile = async () => {[m
[32m+[m[32m    if (!profile) return;[m
[32m+[m[32m    setSavingProfile(true);[m
[32m+[m[32m    setMessage(null);[m
[32m+[m[32m    setError(null);[m
[32m+[m[32m    const updated = await usersApi.update(profile.id, {[m
[32m+[m[32m      firstName: profile.firstName,[m
[32m+[m[32m      lastName: profile.lastName,[m
[32m+[m[32m      gender: profile.gender ?? undefined,[m
[32m+[m[32m    });[m
[32m+[m[32m    if (updated) {[m
[32m+[m[32m      setProfile(updated);[m
[32m+[m[32m      setMessage('Profile updated successfully');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      setError('Failed to update profile');[m
[32m+[m[32m    }[m
[32m+[m[32m    setSavingProfile(false);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleChangePassword = async () => {[m
[32m+[m[32m    setChangingPassword(true);[m
[32m+[m[32m    setMessage(null);[m
[32m+[m[32m    setError(null);[m
[32m+[m[32m    if (passwordForm.newPassword !== passwordForm.confirmationPassword) {[m
[32m+[m[32m      setError('New password and confirmation do not match');[m
[32m+[m[32m      setChangingPassword(false);[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    const ok = await usersApi.changePassword(passwordForm);[m
[32m+[m[32m    if (ok) {[m
[32m+[m[32m      setMessage('Password changed successfully');[m
[32m+[m[32m      setPasswordForm({[m
[32m+[m[32m        currentPassword: '',[m
[32m+[m[32m        newPassword: '',[m
[32m+[m[32m        confirmationPassword: '',[m
[32m+[m[32m      });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      setError('Failed to change password');[m
[32m+[m[32m    }[m
[32m+[m[32m    setChangingPassword(false);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="flex h-screen bg-gray-50">[m
[32m+[m[32m      <Sidebar[m
[32m+[m[32m        isOpen={isMobileMenuOpen}[m
[32m+[m[32m        onClose={() => setIsMobileMenuOpen(false)}[m
[32m+[m[32m      />[m
[32m+[m
[32m+[m[32m      <div className="flex-1 flex flex-col overflow-hidden">[m
[32m+[m[32m        <Header[m
[32m+[m[32m          title="Settings"[m
[32m+[m[32m          onMobileMenuToggle={() => setIsMobileMenuOpen(true)}[m
[32m+[m[32m        />[m
[32m+[m
[32m+[m[32m        <main className="flex-1 overflow-y-auto">[m
[32m+[m[32m          <div className="p-4 sm:p-6 max-w-5xl mx-auto space-y-6">[m
[32m+[m[32m            {message && ([m
[32m+[m[32m              <div className="bg-green-50 text-green-800 px-4 py-3 rounded">[m
[32m+[m[32m                {message}[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m[32m            {error && ([m
[32m+[m[32m              <div className="bg-red-50 text-red-800 px-4 py-3 rounded">[m
[32m+[m[32m                {error}[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m
[32m+[m[32m            {/* Profile Section */}[m
[32m+[m[32m            <section className="bg-white rounded-lg shadow-sm border border-gray-200">[m
[32m+[m[32m              <div className="px-6 py-4 border-b border-gray-200">[m
[32m+[m[32m                <h2 className="text-lg font-semibold">Profile</h2>[m
[32m+[m[32m                <p className="text-sm text-gray-500">[m
[32m+[m[32m                  Update your account information[m
[32m+[m[32m                </p>[m
[32m+[m[32m              </div>[m
[32m+[m[32m              <div className="p-6 space-y-4">[m
[32m+[m[32m                {loading ? ([m
[32m+[m[32m                  <div>Loading...</div>[m
[32m+[m[32m                ) : profile ? ([m
[32m+[m[32m                  <>[m
[32m+[m[32m                    <div className="grid grid-cols-1 sm:grid-cols-2 gap-4">[m
[32m+[m[32m                      <div>[m
[32m+[m[32m                        <label[m
[32m+[m[32m                          htmlFor="firstName"[m
[32m+[m[32m                          className="block text-sm text-gray-700 mb-1"[m
[32m+[m[32m                        >[m
[32m+[m[32m                          First Name[m
[32m+[m[32m                        </label>[m
[32m+[m[32m                        <input[m
[32m+[m[32m                          type="text"[m
[32m+[m[32m                          id="firstName"[m
[32m+[m[32m                          value={profile.firstName || ''}[m
[32m+[m[32m                          onChange={(e) =>[m
[32m+[m[32m                            handleProfileChange('firstName', e.target.value)[m
[32m+[m[32m                          }[m
[32m+[m[32m                          className="w-full border rounded px-3 py-2"[m
[32m+[m[32m                        />[m
[32m+[m[32m                      </div>[m
[32m+[m[32m                      <div>[m
[32m+[m[32m                        <label[m
[32m+[m[32m                          htmlFor="lastName"[m
[32m+[m[32m                          className="block text-sm text-gray-700 mb-1"[m
[32m+[m[32m                        >[m
