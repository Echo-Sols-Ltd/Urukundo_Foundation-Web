[32m+[m[32m                        </label>[m
[32m+[m[32m                        <select[m
[32m+[m[32m                          id="gender"[m
[32m+[m[32m                          value={profile.gender || ''}[m
[32m+[m[32m                          onChange={(e) =>[m
[32m+[m[32m                            handleProfileChange('gender', e.target.value)[m
