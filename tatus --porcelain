[32m+[m[32mimport { createServer } from 'node:http';[m
[32m+[m[32mimport next from 'next';[m
[32m+[m
[32m+[m[32mconst port = process.env.PORT || 8080;[m
[32m+[m[32mconst hostname = process.env.HOSTNAME || '0.0.0.0';[m
[32m+[m
[32m+[m[32mconsole.log(`Starting server on ${hostname}:${port}`);[m
[32m+[m[32mconsole.log('Environment variables:', {[m
[32m+[m[32m  PORT: process.env.PORT,[m
[32m+[m[32m  HOSTNAME: process.env.HOSTNAME,[m
[32m+[m[32m  NODE_ENV: process.env.NODE_ENV[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mconst app = next({[m[41m [m
[32m+[m[32m  dev: false,[m
[32m+[m[32m  hostname,[m
[32m+[m[32m  port,[m
[32m+[m[32m  dir: '.'[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mconst handle = app.getRequestHandler();[m
[32m+[m
[32m+[m[32mapp.prepare().then(() => {[m
[32m+[m[32m  const server = createServer(async (req, res) => {[m
[32m+[m[32m    try {[m
[32m+[m[32m      await handle(req, res);[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      console.error('Error occurred handling', req.url, err);[m
[32m+[m[32m      res.statusCode = 500;[m
[32m+[m[32m      res.end('internal server error');[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  server.listen(port, hostname, (err) => {[m
[32m+[m[32m    if (err) throw err;[m
[32m+[m[32m    console.log(`> Ready on http://${hostname}:${port}`);[m
[32m+[m[32m  });[m
[32m+[m[32m}).catch((ex) => {[m
[32m+[m[32m  console.error('Error starting server:', ex);[m
[32m+[m[32m  process.exit(1);[m
[32m+[m[32m});[m
\ No newline at end of file[m
[1mdiff --git a/frontend/src/app/admin/settings/page.tsx b/frontend/src/app/admin/settings/page.tsx[m
